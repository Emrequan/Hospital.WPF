<Application x:Class="HospitalIntegrationApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conv="clr-namespace:HospitalIntegrationApp.Converters"
             StartupUri="LoginWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/HospitalIntegrationApp;component/Themes/Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <conv:TagToImageConverter x:Key="TagToImageConverter"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>

    


